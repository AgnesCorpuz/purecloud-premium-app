{
    "groups": ["agents", "supervisors", "admin"],
    "roles": [
        {
            "name": "admin",
            "permissions": ["admin", "premium_app_permission"]
        },
        {
            "name": "supervisor",
            "permissions": ["premium_app_permission"]
        },
        {
            "name": "agent",
            "permissions": ["premium_app_permission"]
        }
    ],
    "appInstances": [
        {
            "name": "Agent Widget",
            "url": "https://localhost/index.html",
